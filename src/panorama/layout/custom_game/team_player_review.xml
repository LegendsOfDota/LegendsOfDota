<root>
    <styles>
        <include src="file://{resources}/styles/custom_game/game_setup.css" />
        <include src="file://{resources}/styles/custom_game/shared/shared.css" />
    </styles>
    <scripts>
        <include src="file://{resources}/scripts/custom_game/team_player_review.js" />
    </scripts>
    <Panel class="reviewPlayerContainer">
        <Panel>
            <DOTAAvatarImage id="reviewPhasePlayerAvatarBig" />
            <Panel id="reviewPhaseHeroImageContainer" />
            <Image id="playerAttribute" src="file://{images}/primary_attribute_icons/primary_attribute_icon_strength.psd" class="reviewPhasePlayerAttribute doNotShow" />
            <Label class="lodPlayerReadyText" text="Ready" hittest="false" />
        </Panel>

        <Panel id="reviewPhasePlayerSkillContainer">
            <DOTAAbilityImage id="playerSkill1" abilityname="life_stealer_empty_1" />
            <DOTAAbilityImage id="playerSkill2" abilityname="life_stealer_empty_1" />
            <DOTAAbilityImage id="playerSkill3" abilityname="life_stealer_empty_1" />
            <DOTAAbilityImage id="playerSkill4" abilityname="life_stealer_empty_1" />
            <DOTAAbilityImage id="playerSkill5" abilityname="life_stealer_empty_1" />
            <DOTAAbilityImage id="playerSkill6" abilityname="life_stealer_empty_1" />
        </Panel>

        <Panel class="playerNameContainer">
            <DOTAAvatarImage id="reviewPhasePlayerAvatar" />
            <Label id="playerName" text="PLAYER_NAME" />
        </Panel>
    </Panel>
</root>
